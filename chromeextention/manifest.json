{
    "manifest_version": 3,
    "name": "Age Calculator",
    "version": "1.0",
    "description": "Shows the user's age on every new tab",
    "permissions": ["storage"],
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  